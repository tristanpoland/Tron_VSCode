{
  "comments": {
    "lineComment": "//",
    "blockComment": ["/*", "*/"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["@[", "]@"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["`", "`"],
    ["@[", "]@"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["`", "`"],
    ["@[", "]@"]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^(.*\\{[^}\"'`]*|.*(\\(|\\[)[^\\)\\]\"'`]*)$",
    "decreaseIndentPattern": "^(.*\\*\\/)?\\s*((\\})|(\\)+[;,]?)|(\\][;,]?)).*$"
  },
  "folding": {
    "markers": {
      "start": "^\\s*//\\s*#region",
      "end": "^\\s*//\\s*#endregion"
    }
  }
}